{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}